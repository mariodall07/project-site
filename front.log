<div class="content-wrapper">
<h2>Scroll Down</h2>
    <section class="primary" id="portfolio">
        <div class="container">
            <div class="row">
			 
                <div class="col-lg-12 text-center">
                   
                    
                </div>
            </div>
            <div class="row">
			
			<p class="lead"><a class="btn btn-lg btn-info" href="3.log">Characters</a></p>
                <div class="col-sm-4">
				
                  <img src="assets/1.png">    
                </div>
					<p class="lead"><a class="btn btn-lg btn-info" href="arcs.log">Chapters</a></p>
                <div class="col-sm-4">
                    <img src="http://img-cache.cdn.gaiaonline.com/c0045c7a7c04f4b26281da289e8799d8/http://i246.photobucket.com/albums/gg91/_McLovin/Naruto_Shippuden2_mediano.png">    
